<module>
  <inherits name="com.google.gwt.core.Core" />

  <define-configuration-property name="dagger.module" is-multi-valued="false" />

  <source path="">
    <exclude name="**/codegen/**" />
    <exclude name="**/plugins/**" />
  </source>

  <super-source path="override" />

  <generate-with class="dagger.internal.codegen.PluginGenerator">
    <when-type-assignable class="dagger.internal.Plugin" />
  </generate-with>
</module>